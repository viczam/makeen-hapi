{
  "name": "makeen-core",
  "version": "0.5.16",
  "description": "Makeen core plugin",
  "main": "build/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/makeen-project/makeen/tree/develop/packages/core"
  },
  "scripts": {
    "test": "jest --silent=true --color  --collectCoverageFrom=src/** --runInBand --testPathPattern=/__tests__/.*\\.test\\.js$",
    "test:watch": "jest --watch --silent=true --color  --collectCoverageFrom=src/** --runInBand --testPathPattern=/__tests__/.*\\.test\\.js$",
    "coverage": "jest --silent=false --colors --coverage  --collectCoverageFrom=src/** --runInBand --testPathPattern=/__tests__/.*\\.test\\.js$",
    "coverage:watch": "jest --silent=false --colors --coverage  --collectCoverageFrom=src/** --runInBand --testPathPattern=/__tests__/.*\\.test\\.js$"
  },
  "keywords": [
    "makeen",
    "octobus"
  ],
  "author": "Victor Zamfir <victor@makeen.io>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/makeen-project/makeen/issues"
  },
  "homepage": "https://github.com/makeen-project/makeen/blob/develop/packages/core/readme.md",
  "dependencies": {
    "axios": "0.15.3",
    "bluebird": "^3.5.0",
    "boom": "4.2.0",
    "environment-override": "1.0.2",
    "glue": "4.1.0",
    "hapi-boom-decorators": "2.2.2",
    "hapi-octobus": "0.9.0",
    "joi": "10.2.2",
    "lodash": "^4.11.1",
    "makeen-db": "^0.4.1",
    "makeen-router": "^0.5.2",
    "mongodb": "2.2.24",
    "mongodb-extended-json": "1.9.0",
    "octobus-crud": "0.5.0",
    "octobus-mongodb-store": "0.4.1",
    "octobus.js": "0.31.0",
    "scooter": "4.0.0"
  },
  "devDependencies": {
    "confidence": "^3.0.2",
    "jest": "^19.0.2"
  }
}
