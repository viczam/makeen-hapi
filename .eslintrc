{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "globals": {
    "__DEV__": true,
    "jest": true,
    "expect": true,
    "beforeAll": true,
    "afterAll": true
  },
  "env": {
    "es6": true,
    "browser": true,
    "mocha": true,
    "node": true
  },
  "rules": {
    "no-underscore-dangle": ["error", { "allow": ["_id"] }],
    "arrow-parens": 0,
    "import/prefer-default-export": 0,
    "react/forbid-prop-types": 0,
    "jsx-a11y/label-has-for": 0,
    "react/no-danger": 0
  }
}
